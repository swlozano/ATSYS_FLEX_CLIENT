                    getdocs(modSiPresentHv,RecursoHumanoMl.getInstance().RecursoHumanoPpl.dgrecursohumano.selectedItem.presntoHv),
                    getdocs(modSiPresentEntrevista,RecursoHumanoMl.getInstance().RecursoHumanoPpl.dgrecursohumano.selectedItem.presentoEntrevista)
                   ,RecursoHumanoMl.getInstance().ModidCiudadNat,RecursoHumanoMl.getInstance().ModidCiudadExp,
                    getafil(modARPrechumano,RecursoHumanoMl.getInstance().RecursoHumanoPpl.dgrecursohumano.selectedItem.arp.id),
                    getafil(modEPSrechumano,RecursoHumanoMl.getInstance().RecursoHumanoPpl.dgrecursohumano.selectedItem.eps.id),
                    getafil(modAFPrechumano,RecursoHumanoMl.getInstance().RecursoHumanoPpl.dgrecursohumano.selectedItem.afp.id));
                   var evento:RecursoHumanoEvent =new RecursoHumanoEvent(RecursoHumanoEvent.MODIFICAR_RECURSO_HUMANO,recurso,null);
