                        }
                   
                      
                   var recurso:RecursoHumanoVO = new RecursoHumanoVO(RecursoHumanoMl.getInstance().RecursoHumanoPpl.dgrecursohumano.selectedItem.id,modNomrechumano.text,modAperechumano.text,parseInt(modCedrechumano.text),modemailrechumano.text,
