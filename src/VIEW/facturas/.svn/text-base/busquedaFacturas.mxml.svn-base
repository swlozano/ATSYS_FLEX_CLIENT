            private var focussedFormControl:DisplayObject;
            
            private function validateForm(event:Event):void{
            
               focussedFormControl = event.target as DisplayObject;
               formIsValid = true;
               
               if(viewStackPeriodos.selectedIndex==0){
               
	               validate(yearInicioAnualValidator);
	               validate(yearFinAnualValidator);
               
               }else if(viewStackPeriodos.selectedIndex==1){
               	
               	        validate(yearInicioSemestralValidator);
	                    validate(yearFinSemestralValidator);
                	
                       }else if(viewStackPeriodos.selectedIndex==2){
               	
		               	        validate(yearInicioBimensualValidator);
			                    validate(yearFinBimensualValidator);
		                	
		                       }else if(viewStackPeriodos.selectedIndex==3){
               	
				               	         validate(yearInicioMensualValidator);
					                     validate(yearFinMensualValidator);
		                	
		                               }else if(viewStackPeriodos.selectedIndex==4){
               	
						               	         validate(fechaInicioValidator);
							                     validate(fechaFinValidator);
				                	
				                               }
               
            }
            
            private function validate(validator:Validator):Boolean{
            
               var validatorSource:DisplayObject = validator.source as DisplayObject;
               var suppressEvents:Boolean = (validatorSource != focussedFormControl);
               var event:ValidationResultEvent = validator.validate(null, suppressEvents); 
               var currentControlIsValid:Boolean = (event.type == ValidationResultEvent.VALID);
               formIsValid = formIsValid && currentControlIsValid;
               return currentControlIsValid;
            }
            
