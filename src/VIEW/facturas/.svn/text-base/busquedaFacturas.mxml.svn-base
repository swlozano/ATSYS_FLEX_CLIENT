           public function ObtenerFechasPeriodoAnual(tipoyear:int,year:int):Date{
           	
           	 var retorno:Date;
           	 var dia:int;
           	 var mes:int; 
           	
           	
           	  if(tipoyear==1){
           	  	
           	  	mes=1;
           	  	dia=1;
           	  	
           	  	retorno = new Date(mes+"/"+dia+"/"+year);
           	  	
           	  }else{
           	  	
           	  	     mes=12;
           	  	     dia=31;
           	  	     
           	  	     retorno = new Date(mes+"/"+dia+"/"+year);
           	  	
           	       } 
           	       
           	       return retorno;
           	
           } 
            
            public function ObtenerFechasPeriodoBimensual(tipofecha:int,year:int,periodo:int):Date{
            	
            	
            	var retorno:Date;
            	var mes:int;
            	var dia:int;
            	
            	
            	if(tipofecha==1){
            	
            	   switch(periodo){
            	   	
            	   	  case 1:
            	   	  
            	   	     mes=1;
            	   	     dia=1;
            	   	     retorno = new Date(mes+"/"+dia+"/"+year);
            	   	  
            	   	  break;
            	   	  
            	   	  case 2:
            	   	  
            	   	     mes=3;
            	   	     dia=1;
            	   	     retorno = new Date(mes+"/"+dia+"/"+year);
            	   	  
            	   	  break;
            	   	  
            	   	  case 3:
            	   	   
            	   	     mes=5;
            	   	     dia=1;
            	   	     retorno = new Date(mes+"/"+dia+"/"+year);
            	   	    
            	   	  break;
            	   	  
            	   	  case 4:
            	   	  
            	   	     mes=7;
            	   	     dia=1;
            	   	     retorno = new Date(mes+"/"+dia+"/"+year);
            	   	  
            	   	  break;
            	   	  
            	   	  case 5:
            	   	  
            	   	     mes=9;
            	   	     dia=1;
            	   	     retorno = new Date(mes+"/"+dia+"/"+year);
            	   	      
            	   	  break;
            	   	  
            	   	  case 6:
            	   	  
            	   	     mes=11;
            	   	     dia=1;
            	   	     retorno = new Date(mes+"/"+dia+"/"+year);
            	   	  
            	   	  break;
            	   	  
            	   	
            	   }	
            		
            	}else if(tipofecha==2){
            		
            		     switch(periodo){
            	   	
			            	   	  case 1:
			            	   	  
			            	   	     mes=2;
			            	   	     
			            	   	     if(year%4==0){
			            	   	     	
			            	   	     	dia=29;
			            	   	     	
			            	   	     }else{
			            	   	     	
			            	   	     	   dia=28;
			            	   	     	
			            	   	          }
			            	   	     
			            	   	     retorno = new Date(mes+"/"+dia+"/"+year);
			            	   	  
			            	   	  break;
			            	   	  
			            	   	  case 2:
			            	   	  
			            	   	     mes=4;
			            	   	     dia=30;
			            	   	     retorno = new Date(mes+"/"+dia+"/"+year);
			            	   	  
			            	   	  break;
			            	   	  
			            	   	  case 3:
			            	   	   
			            	   	     mes=6;
			            	   	     dia=30;
			            	   	     retorno = new Date(mes+"/"+dia+"/"+year);
			            	   	    
			            	   	  break;
			            	   	  
			            	   	  case 4:
			            	   	  
			            	   	     mes=8;
			            	   	     dia=30;
			            	   	     retorno = new Date(mes+"/"+dia+"/"+year);
			            	   	  
			            	   	  break;
			            	   	  
			            	   	  case 5:
			            	   	  
			            	   	     mes=10;
			            	   	     dia=31;
			            	   	     retorno = new Date(mes+"/"+dia+"/"+year);
			            	   	      
			            	   	  break;
			            	   	  
			            	   	  case 6:
			            	   	  
			            	   	     mes=12;
			            	   	     dia=31;
			            	   	     retorno = new Date(mes+"/"+dia+"/"+year);
			            	   	  
			            	   	  break;
			            	   	  
			            	   	
			            	   }
            		
            	        }
            	        
            	        return retorno;
            	
            }
            
            
            public function ObtenerFechasPeriodoMensual(tipofecha:int,year:int,mes:int):Date{
            	
            	var retorno:Date;
            	var dia:int;
            	
            	if(tipofecha==1){
            		
            		  dia=1;
            		  
            		  retorno = new Date(mes+"/"+dia+"/"+year);
            		   
            		
            	  }else if(tipofecha==2){
            		
                                    	
                             if(mes == 1 || mes == 3 || mes == 5 || mes == 7 || mes == 8 || mes == 10 || mes == 12){
                             	
                             	dia=31;
                             	
                             }else if(mes == 4 || mes == 6 || mes == 9 || mes == 11){
                             	
                                      dia=30;
                                     
                                     }else if(mes==2){
                                     	
                                               if(year %4==0){
                                               	
                                                    dia=29;
                                                 
                                                 }else{
                                                 	   
                                                 	   dia=28;
                                                 	   
                                                      }
                                             
                                             }
                                                    	
                               retorno = new Date(mes+"/"+dia+"/"+year);		
            		
            	           }
            	
            	return retorno;
            	
            }
            
            public function ObtenerFechasPeriodoSemestral(tipofecha:int,year:int,periodo:int):Date{
            	
            	var retorno:Date;
            	var mes:int;
            	var dia:int;
            	            	
            	if(tipofecha==1){
            		
            		if(periodo==1){
            			
            			mes=1;
            			dia=1;
            			retorno = new Date(mes+"/"+dia+"/"+year);
            			
            		}else{
            			
            		       mes=7;
            			   dia=1;
            			   retorno = new Date(mes+"/"+dia+"/"+year);
            		     
            		     }
            		
            	}else{
            		
            	        if(periodo==1){
            			
            			  mes=6;
            			  dia=30;
            			  var str:String =6+"/"+dia+"/"+year;
            			  retorno = new Date(str);
            			
	            		}else{
	            			
	            		       mes=12;
	            			   dia=31;
	            			   retorno = new Date(mes+"/"+dia+"/"+year);
	            		     
	            		     }
            	       
            	     }
            	     
            	     
            	     return retorno;
            	
            }
            
            
