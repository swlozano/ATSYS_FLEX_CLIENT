package VO
{
	import com.adobe.cairngorm.vo.IValueObject;

	public class FechasCorteCausacionesVO implements IValueObject
	{
		
		public var id:int;
		public var fechaCorte:Date;
		public var yearCausado:int; 
		public var mesCausado:int;
		public var tipoCausacion:int;
		public var asignado:Boolean;
		
		
		public function FechasCorteCausacionesVO(
		
		 id:int,
		 fechaCorte:Date,
		 yearCausado:int,
		 mesCausado:int,
		 tipoCausacion:int,
		 asignado:Boolean 
		
		)
		{
			
			this.id=id;
			this.fechaCorte=fechaCorte;
			this.yearCausado=yearCausado;
			this.mesCausado=mesCausado;
			this.tipoCausacion=tipoCausacion;
			this.asignado = asignado;
			
		}

	}
}