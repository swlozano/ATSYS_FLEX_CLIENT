package VO
{
	import com.adobe.cairngorm.vo.ValueObject;

	public class PermisosVO implements ValueObject
	{
		public var id:int;
		public var nombre:String;
		public var asignado:Boolean;
		
		public function PermisosVO(id:int,nombre:String,asignado:Boolean)
		{
			this.id=id;
			this.nombre=nombre;
			this.asignado =asignado;
		}

	}
}