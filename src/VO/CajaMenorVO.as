package VO
{
	import com.adobe.cairngorm.vo.ValueObject;
	
    [Bindable]
	public class CajaMenorVO implements ValueObject
	{
		public var id:int;
		public var id_responsable:int;	
		public var nombre:String;
		public var monto_inicio:Number;
		public var valorMinimoNotificacion:Number;
		public var montoMinimoReintegro:Number;
		public var idUsuariosToNotificar:Array;
		public var idCentrocosto:int;
		public var idPersona:int;
		public var esCuenta:Number;
		public var asignado:Boolean;
		
		public function CajaMenorVO(
			id:int,
		 	id_responsable:int,	
	     	nombre:String,
			monto_inicio:Number,
			valorMinimoNotificacion:Number,
			montoMinimoReintegro:Number,
			idUsuariosToNotificar:Array,
			idCentrocosto:int,
			idPersona:int,
			esCuenta:Number,
			asignado:Boolean
			)
		{
			this.id=id;
			this.id_responsable=id_responsable;
			this.nombre=nombre;
			this.monto_inicio=monto_inicio;
			this.valorMinimoNotificacion=valorMinimoNotificacion;
			this.montoMinimoReintegro=montoMinimoReintegro;
			this.idUsuariosToNotificar=idUsuariosToNotificar;
			this.idCentrocosto=idCentrocosto;
			this.idPersona=idPersona;
			this.esCuenta=esCuenta;
			this.asignado=asignado;
		}

	}
}