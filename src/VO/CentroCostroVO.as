package VO
{
	import com.adobe.cairngorm.vo.ValueObject;

	public class CentroCostroVO implements ValueObject
	{
		public var id:int;
		public var nombre:String;
		public var ciudad:String;
		public var asignado:Boolean;
		
		public function CentroCostroVO(id:int,nombre:String,ciudad:String,asignado:Boolean)
		{
			this.id=id;
			this.nombre=nombre;
			this.ciudad=ciudad;
		    this.asignado=asignado;
		}

	}
}