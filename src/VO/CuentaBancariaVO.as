package VO
{
	import com.adobe.cairngorm.vo.ValueObject;

	public class CuentaBancariaVO implements ValueObject
	{
		public var id:int;
		public var idBanco:int;
		public var idtipoCuenta:int;
		public var numeroCuenta:String;
		
		public function CuentaBancariaVO(
		
		  id:int,
		  idBanco:int,
		  idtipoCuenta:int,
		  numeroCuenta:String
		
		)
		
		{
			this.id = id;
			this.idBanco = idBanco;
			this.idtipoCuenta = idtipoCuenta;
			this.numeroCuenta = numeroCuenta;
		}

	}
}