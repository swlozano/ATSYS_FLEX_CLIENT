package VO
{
	import com.adobe.cairngorm.vo.IValueObject;

	public class CuentaPorPagarVO implements IValueObject
	{
		
		public var id:int;
		public var fechaOrigen:Date;
		public var idRecursoHumano:int;
		public var idPersona:int;
		public var concepto:String;
		public var idCuentaBancaria:int;
		public var idBanco:int;
		public var idTipoCuentaBanco:int;
		public var numeroCuenta:String;
		public var valor:Number;
		public var idEstadoCuentaPPagar:int;
		public var idGeneradorCuentaPPagar:int;
		public var idCampo:int;
		public var fechaPago:Date;
		
		
		
		public function CuentaPorPagarVO(
		
		    id:int,
			fechaOrigen:Date,
			idRecursoHumano:int,
			idPersona:int,
			concepto:String,
			idCuentaBancaria:int,
			idBanco:int,
			idTipoCuentaBanco:int,
			numeroCuenta:String,
			valor:Number,
			idEstadoCuentaPPagar:int,
			idGeneradorCuentaPPagar:int,
			idCampo:int,
			fechaPago:Date

		)
		{
			
			this.id = id;
			this.fechaOrigen = fechaOrigen;
			this.idRecursoHumano = idRecursoHumano;
			this.idPersona = idPersona;
			this.concepto = concepto;
            this.idCuentaBancaria = idCuentaBancaria;
            this.idBanco = idBanco;
            this.idTipoCuentaBanco = idTipoCuentaBanco;
            this.numeroCuenta = numeroCuenta;
            this.valor = valor;
            this.idEstadoCuentaPPagar = idEstadoCuentaPPagar;
            this.idGeneradorCuentaPPagar = idGeneradorCuentaPPagar;
            this.idCampo = idCampo;
            this.fechaPago = fechaPago;			
			
		}

	}
}







