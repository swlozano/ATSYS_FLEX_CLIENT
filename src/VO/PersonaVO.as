package VO
{
	import com.adobe.cairngorm.vo.ValueObject;
    
    [Bindable]
	public class PersonaVO implements ValueObject
	{
		public var id:int;
		public var nit:Number;
		public var cedula:int;
		public var telefono:String;
		public var nombreRs:String;
		public var asignado:Boolean;
		  
		
		public function PersonaVO(
		     id:int,
		     nit:Number,
		     cedula:int,
		     telefono:String,
		     nombreRs:String,
		     asignado:Boolean)
		{
			 this.id=id;
			 this.nit=nit;
			 this.cedula=cedula;
			 this.telefono=telefono;
			 this.nombreRs=nombreRs;
			 this.asignado=asignado;
		}

	}
}